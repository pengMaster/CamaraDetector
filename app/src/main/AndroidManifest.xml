<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="king.bird.camaradetector">

    <!-- 读 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!-- 联网 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 联系人 -->
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 短信 -->
    <uses-permission android:name="android.permission.READ_SMS" />
    <!-- 通话记录 -->
    <uses-permission android:name="android.permission.READ_CALL_LOG" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_camera"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_camera"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".act.TestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".act.MainActivity" />
        <!--<activity android:name=".act.TestActivity"></activity>-->
    </application>

</manifest>